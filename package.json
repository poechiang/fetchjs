  "module": "./dist/index.es.js",
  "packageManager": "yarn@4.0.2",
  "types": "./dist/index.d.ts",
  "description": "对fetch api 封装",
  "keywords": [
    "fetch",
    "web",
    "node"
  ],
  "author": "jeffrey·chiang",
